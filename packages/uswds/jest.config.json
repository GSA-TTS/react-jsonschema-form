{
  "rootDir": ".",
  "testEnvironment": "jsdom",
  "testMatch": ["**/test/**/*.test.ts?(x)", "**/test/**/*.spec.ts?(x)", "**/test/test.ts?(x)"],
  "transform": {
    "^.+\\.(ts|tsx|js|jsx)$": ["babel-jest", { "presets": ["@babel/preset-env", "@babel/preset-react", "@babel/preset-typescript"] }]
  },
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json"],
  "moduleNameMapper": {
    "^@rjsf/core/(.*)$": "<rootDir>/../core/src/$1",
    "^@rjsf/core$": "<rootDir>/../core/src",
    "^@rjsf/utils/(.*)$": "<rootDir>/../utils/src/$1",
    "^@rjsf/utils$": "<rootDir>/../utils/src",
    "^@rjsf/validator-ajv8/(.*)$": "<rootDir>/../validator-ajv8/src/$1",
    "^@rjsf/validator-ajv8$": "<rootDir>/../validator-ajv8/src",
    "^@rjsf/snapshot-tests/(.*)$": "<rootDir>/test/setupForTests.tsx",
    "^@rjsf/snapshot-tests$": "<rootDir>/test/setupForTests.tsx",
    "\\.(css|less|scss|sass)$": "identity-obj-proxy"
  },
  "transformIgnorePatterns": [
    "/node_modules/(?!(@rjsf|nanoid)/)"
  ],
  "setupFilesAfterEnv": [],
  "setupFiles": ["<rootDir>/jest.setup.js"],
  "collectCoverageFrom": [
    "src/**/*.{js,jsx,ts,tsx}",
    "!src/**/*.d.ts"
  ],
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/test/"
  ],
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/dist/",
    "/lib/"
  ],
  "passWithNoTests": true
}
